-- åˆ·æ–°æ•°æ®æŒ‰é’®
createMobileButton(" åˆ·æ–°æ•°æ®", "æ‰‹åŠ¨åˆ·æ–°ç»Ÿè®¡æ•°æ®", function()
    -- æ‰‹åŠ¨è§¦å‘æ•°æ®æ›´æ–°
    lastStrength = 0
    strengthHistory = {}
end)

-- ==================== æ•°æ®æ›´æ–°åç¨‹ ====================
spawn(function()
    while screenGui and screenGui.Parent do
        task.wait(1)  -- æ¯ç§’æ›´æ–°ä¸€æ¬¡
        
        pcall(function()
            -- æ›´æ–°åŠ›é‡
            local currentStrength = player.leaderstats.Strength.Value
            dataLabels.strength.Text = formatNumber(currentStrength)
            dataLabel.Text = "åŠ›é‡: " .. formatNumber(currentStrength)
            
            -- æ›´æ–°è€ä¹…åº¦
            local durability = getDurability()
            dataLabels.durability.Text = formatNumber(durability)
            
            -- è®¡ç®—åŠ›é‡å¢é•¿
            if lastStrength > 0 then
                local gain = currentStrength - lastStrength
                local gainPerSecond = gain
                
                table.insert(strengthHistory, gainPerSecond)
                if #strengthHistory > 5 then
                    table.remove(strengthHistory, 1)
                end
                
                -- è®¡ç®—å¹³å‡é€Ÿåº¦
                local totalGain = 0
                for _, g in ipairs(strengthHistory) do
                    totalGain = totalGain + g
                end
                local avgSpeed = #strengthHistory > 0 and totalGain / #strengthHistory or 0
                dataLabels.speed.Text = string.format("%.2f/s", avgSpeed)
                
                -- æ›´æ–°æ¯æ—¥å¢é•¿
                local dailyGain = avgSpeed * 86400
                dataLabel.Text = "åŠ›é‡: " .. formatNumber(currentStrength) .. " (+" .. formatNumber(dailyGain) .. "/å¤©)"
            else
                dataLabels.speed.Text = "0/s"
            end
            
            lastStrength = currentStrength
            
            -- æ›´æ–°è›‹ç™½è›‹çŠ¶æ€
            local hasProtein = false
            if player.boostTimersFolder then
                hasProtein = player.boostTimersFolder:FindFirstChild("Protein Egg") ~= nil
            end
            dataLabels.protein.Text = hasProtein and "Yes æ¿€æ´»" or "No æœªæ¿€æ´»"
        end)
    end
end)

print("ğŸ“± ç§»åŠ¨ç‰ˆWindUIå·²åŠ è½½ï¼")
print("âœ… æ”¯æŒæ‰‹æœºè§¦æ‘¸æ‹–åŠ¨")
print("ğŸ“Š å®æ—¶æ•°æ®æ˜¾ç¤º")
print("âš¡ æé€Ÿé”»ç‚¼åŠŸèƒ½å°±ç»ª")
